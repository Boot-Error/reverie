{
  "manifest_version": 3,
  "name": "Reverie",
  "version": "0.0.1",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "static/js/background.js",
    "type": "module"
  },
  "permissions": ["storage", "tabs", "history", "scripting", "activeTab"],
  "host_permissions": ["<all_urls>"]
}
